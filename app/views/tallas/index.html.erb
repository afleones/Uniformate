<% breadcrumb :tallas %>
<div class="table-responsive">
  <h1 class="text-center">Tallas</h1><br>
    <div class="col-md-6">
      <table class="table table-striped table-bordered">
        <tr>
          <th class="bg-light">ID</th>
          <th class="bg-light">NOMBRE</th>
        </tr>
        <% @tallas.each do |talla| %>
          <tr>
            <td><%= link_to talla.id , talla%></td>
            <td><%= talla.nombre %></td>
          </tr>
        <%end%>
      </table>
    </div>
    <div class="col-md-6">
    </div>
</div>
